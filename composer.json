{
    "name": "your-name-here/MultipartJsonApiListener",
    "description": "MultipartJsonApiListener plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4",
        "friendsofcake/crud-json-api": "^0.2.2",
        "riverline/multipart-parser": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MultipartJsonApiListener\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MultipartJsonApiListener\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
